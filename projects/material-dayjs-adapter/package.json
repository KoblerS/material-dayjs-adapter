{
  "name": "material-dayjs-adapter",
  "version": "1.0.3",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy": "../../node_modules/.bin/cpy schematics/**/{collection.json,schema.json,files/**} ../../dist/material-dayjs-adapter --parents",
    "postbuild": "npm run copy",
    "clean:dist": "rm -rf ../../dist/material-dayjs-adapter"
  },
  "author": {
    "name": "Simon Kobler",
    "url": "https://github.com/koblers/"
  },
  "description": "A MatDateAdapter for Dayjs to reduce dependency sizes when compared to MomentJS based adapters.",
  "keywords": [
    "angular",
    "angular-material",
    "material",
    "datepicker",
    "mat-datepicker",
    "momentjs",
    "dayjs",
    "adapter",
    "mat-date-adapter",
    "date",
    "iso",
    "utc",
    "typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": ">=14.0.0",
    "@angular/core": ">=14.0.0",
    "@angular/material": ">=14.0.0",
    "dayjs": ">= 1.11.5"
  },
  "repository": {
    "url": "https://github.com/koblers/material-dayjs-adapter"
  },
  "devDependencies": {}
}
